<div class="container-fluid h-100">
  <OrderPad @categories={{this.model.categories}}
            @menuItems={{this.model.menuItems}}
            @editOptions={{this.model.editOptions}}
            @order={{this.model.order}}
            @isNewOrder={{false}}
            @onCancelOrder={{this.unloadStoreAndTransition}}
            @onOrderSubmitted={{this.transitionToOrdersEatIn}}/>
</div>