<div class="orderPad-categories">
  {{#each model.categories}}
    {{category-item category=this action="categoryItemClick"}}
  {{/each}}
</div>
<div class="orderPad-menu">
  {{outlet "menu"}}
</div>
<div class="orderPad-order">
  {{#each model.order.orderItems}}
    <div>
      {{this.quantity}} x {{this.menuItem.name}}
    </div>
  {{/each}}
</div>
{{outlet}}
